on place:
    if {linenumberlog} is not set:
        set {linenumberlog} to "2" parsed as integer
    insert "[now]: %player% placed block %event-block%" at line {linenumberlog} of file path "plugins/logger/block.sk"
    
    
command /resetlogline:
    trigger:
        set {linenumberlog} to "2" parsed as integer
