on place:
    if {linenumberlog} is not set:
        set {filelinenumber} to "2" parsed as integer
    insert "[%now%]: %player% has placed %event-block%." at line {linenumberlog} of file path "plugins/logger/block.txt"
    
command /resetlogline:
    trigger:
        set {linenumberlog} to "2" parsed as integer
